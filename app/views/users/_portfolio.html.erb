<head>   <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

<div class="row">
	                
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
<ul class="collapsible popout" data-collapsible="expandable">
              <span class="card-title">Portfolios</span>
	
	      <ul>  <a class="material-icons small" href="/portfolios">add_box</a>
	      </ul>                                   	                                                                                                  
		<li>                                 
		  <div class="collapsible-header blue-grey darken-1 white-text">Portfolio Name
		    <a class="material-icons small">edit_box</a>                                                                                            
		  </div>                                                                                             
      <div class="collapsible-body  blue-grey lighten-1 white-text"><%= image_tag "album-1.jpg", :alt => "Profile Picture", id:"portfolio-picture"%><span> Enter description</span></div>                                                                                                      
    </li>                                                                                                                                                                              
    <li>                                                                                                                                                                               
      <div class="collapsible-header blue-grey darken-1 white-text">Portfolio Name
	
	<a class="material-icons small" href="/portfolios/edit">edit_box</a>                                                                                            
      </div> 
      <div class="collapsible-body  blue-grey lighten-1 white-text"><%= image_tag "album-2.jpg", :alt => "Profile Picture", id:"portfolio-picture"%><span> Enter description</span></div>                                                                                                      
    </li>

    <% if current_portfolios.any? %>
      <% current_portfolios.find_each do |p| %>
              <li>
		<div class="collapsible-header blue-grey darken-1 white-text"> <%="#{p.name}"%>
		  <a class="material-icons small" href="/portfolios/#{p.id}/edit">edit_box</a>                                                                                            
		</div>                                                                                             
		<div class="collapsible-body  blue-grey lighten-1 white-text"><%= image_tag "album-5.jpg", :alt => "Profile Picture", id:"portfolio-picture"%><span>
		    <%="#{p.description}"%></span>
		   <%= link_to "Go to portfolio", "/portfolios/#{p.id}", target: "_self"%>
		</div>
               
	      </li>
      <% end %>
    <% end%>
</ul>                                                                                                                                                                           
	    </div>
	  </div>
	  </div>
</head>
