<div class="container signup-container row">
  <div class="input-field custom-input-field col s3 offset-s3">
      <input id="first_name" type="text" class="validate">
      <label for="first_name">First Name</label>
  </div>

  <div class="input-field custom-input-field col s3">
      <input id="last_name" type="text" class="validate">
      <label for="last_name">Last Name</label>
  </div>

  <div class="input-field custom-input-field col s3 offset-s3">
      <input id="email" type="text" class="validate">
      <label for="email">Email Address</label>
  </div>

  <div class="input-field custom-input-field col s3">
      <input id="usename" type="text" class="validate">
      <label for="username">Username</label>
  </div>

  <div class="input-field custom-input-field col s4 offset-s4">
      <input id="password" type="password" class="validate">
      <label for="password">Password</label>
  </div>

  <div class="input-field custom-input-field col s4 offset-s4">
      <input id="password" type="password" class="validate">
      <label for="password">Confirm Password</label>
  </div>

  <div class="col s12 center">
    <a><%= link_to "Sign Up", root_path, target: "_self", class: "active waves-effect waves-light btn" %></a>
  </div>
</div>
